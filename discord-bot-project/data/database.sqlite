```sqlite
PRAGMA foreign_keys = ON;
CREATE TABLE IF NOT EXISTS playlists (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT NOT NULL UNIQUE,
  song_id INTEGER,
  FOREIGN KEY (song_id) REFERENCES songs (id)
);
CREATE TABLE IF NOT EXISTS songs (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  title TEXT NOT NULL,
  url TEXT NOT NULL,
  thumbnail TEXT
);
```